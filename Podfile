# platform :ios, '9.0'
project 'LionEngine.xcodeproj'
 
use_frameworks!

target 'Example' do
  pod 'Sourcery'
  pod 'SwiftLint'
end

target 'ECS' do
  # Comment the next line if you don't want to use dynamic frameworks
  pod 'Sourcery'
  pod 'SwiftLint'

  # Pods for ECS

  target 'ECSTests' do
    # Pods for testing
  	inherit! :search_paths
  end

end
